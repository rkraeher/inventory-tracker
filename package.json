    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "bootstrap": "^4.5.3",
    "dotenv": "^16.1.4",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^6.2.4",
    "react-bootstrap": "^1.4.3",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.10.4"
  }
}
